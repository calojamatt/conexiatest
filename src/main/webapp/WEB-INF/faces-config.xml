<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>clientes</from-outcome>
            <to-view-id>/pages/clientes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevocliente</from-outcome>
            <to-view-id>/pages/nuevo_cliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cocineros</from-outcome>
            <to-view-id>/pages/cocineros.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevococinero</from-outcome>
            <to-view-id>/pages/nuevo_cocinero.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>camareros</from-outcome>
            <to-view-id>/pages/camareros.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevocamarero</from-outcome>
            <to-view-id>/pages/nuevo_camarero.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mesas</from-outcome>
            <to-view-id>/pages/mesas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevamesa</from-outcome>
            <to-view-id>/pages/nueva_mesa.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>platos</from-outcome>
            <to-view-id>/pages/platos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevoplato</from-outcome>
            <to-view-id>/pages/nuevo_plato.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>facturas</from-outcome>
            <to-view-id>/pages/facturas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevafactura</from-outcome>
            <to-view-id>/pages/nueva_factura.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>informecamarero</from-outcome>
            <to-view-id>/pages/informe_camarero.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>informeclientes</from-outcome>
            <to-view-id>/pages/informe_clientes.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
